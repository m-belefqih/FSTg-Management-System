# File: .htaccess
RewriteEngine On
RewriteBase /FSTg-Management-System/

# If the request is not an existing file or directory, serve error.php
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . error.php [L]

# Or use Apacheâ€™s built-in 404 handler
ErrorDocument 404 /FSTg-Management-System/error.php